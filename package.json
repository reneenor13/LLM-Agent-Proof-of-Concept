{
  "name": "llm-agent-proof-of-concept",
  "version": "1.0.0",
  "description": "Browser-based LLM Agent with Multi-Tool Reasoning - A proof-of-concept demonstrating LLM-powered agents that can combine text generation with external tools like web search, AI workflows, and code execution",
  "main": "index.html",
  "scripts": {
    "start": "python -m http.server 8000",
    "serve": "live-server .",
    "dev": "live-server --port=3000 --open=index.html",
    "build": "echo 'No build process needed for static HTML' && exit 0",
    "test": "echo 'Run tests by opening test.html in browser'",
    "deploy": "gh-pages -d .",
    "lint": "eslint *.js || echo 'ESLint not installed'",
    "format": "prettier --write *.html *.js *.md || echo 'Prettier not installed'",
    "demo": "echo 'Opening demo...' && python -m http.server 8000"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/reneenor13/LLM-Agent-Proof-of-Concept.git"
  },
  "keywords": [
    "llm",
    "agent",
    "ai",
    "chatbot",
    "openai",
    "tool-calling",
    "browser",
    "javascript",
    "google-search",
    "code-execution",
    "proof-of-concept",
    "multi-tool",
    "reasoning",
    "conversational-ai",
    "bootstrap",
    "frontend",
    "agent-framework",
    "ai-assistant"
  ],
  "author": {
    "name": "reneenor13",
