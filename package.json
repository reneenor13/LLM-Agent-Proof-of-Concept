  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/reneenor13/LLM-Agent-Proof-of-Concept/issues"
  },
  "homepage": "https://reneenor13.github.io/LLM-Agent-Proof-of-Concept",
  "devDependencies": {
    "live-server": "^1.2.2",
    "gh-pages": "^6.0.0",
    "eslint": "^8.52.0",
    "prettier": "^3.0.3"
  },
  "optionalDependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express-rate-limit": "^7.1.4",
    "helmet": "^7.1.0"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ],
